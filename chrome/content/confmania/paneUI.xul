  <preference id="bidi.texttype" name="bidi.texttype" type="int"/>
  <preference id="layout.scrollbar.side" name="layout.scrollbar.side" type="int"/>

  <preference id="general.useragent.locale" name="general.useragent.locale" type="string"/>
  <preference id="intl.locale.matchOS" name="intl.locale.matchOS" type="bool"/>
