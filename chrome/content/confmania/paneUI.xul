  <preference id="layout.enable_japanese_specific_transform" name="layout.enable_japanese_specific_transform" type="bool"/>

  <preference id="ui.click_hold_context_menus" name="ui.click_hold_context_menus" type="bool"/>
  <preference id="ui.submenuDelay" name="ui.submenuDelay" type="int"/>

