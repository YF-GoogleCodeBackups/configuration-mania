<hbox flex="1" orient="horizontal" id="paneUIBox">
<radiogroup id="paneUIBoxTab" class="subtabs"
 orient="vertical" style="max-width: 10em; width: 10em;"
 value="ui-pane-mousewheel" oncommand="gPrefWindow.prefUI.onPaneUIBoxTabSelected();">
  <radio class="subtab" value="ui-pane-mousewheel"  label="&ui.mousewheel.label;"/>
  <radio class="subtab" value="ui-pane-multitouch"  label="&ui.multitouch.label;"/>
  <radio class="subtab" value="ui-pane-findahead"   label="&ui.find.label;"/>
  <radio class="subtab" value="ui-pane-keynav"      label="&ui.keynav.label;"/>
  <radio class="subtab" value="ui-pane-bidi"        label="&ui.bidi.label;"/>
  <radio class="subtab" value="ui-pane-i18n"        label="&ui.i18n.label;"/>
  <radio class="subtab" value="ui-pane-contextmenu" label="&ui.contextmenu.label;"/>
  <radio class="subtab" value="ui-pane-others"      label="&ui.others.label;"/>
</radiogroup>
<splitter/>
<deck id="paneUIBoxContainer" flex="4">
<box class="subtabpanel" orient="vertical" id="ui-pane-mousewheel">
  <groupbox>
    <caption label="&ui.mousewheel.binding.label;"/>

    <data>
      <mousewheelkeies id="ui-pane-data-mousewheelkeies" xmlns="">
        <mousewheelkey name="default"     />
        <mousewheelkey name="with_alt"    />
        <mousewheelkey name="with_control"/>
        <mousewheelkey name="with_meta"   />
        <mousewheelkey name="with_shift"  />
        <mousewheelkey name="with_win"    />
      </mousewheelkeies>
    </data>

    <tabbox flex="1">
      <tabs>
        <tab label="&ui.mousewheel.binding.default.label;"     />
        <tab label="&ui.mousewheel.binding.with_alt.label;"    />
        <tab label="&ui.mousewheel.binding.with_control.label;"/>
        <tab label="&ui.mousewheel.binding.with_meta.label;"   />
        <tab label="&ui.mousewheel.binding.with_shift.label;"  />
        <tab label="&ui.mousewheel.binding.with_win.label;"    />
      </tabs>
      <tabpanels datasources="#ui-pane-data-mousewheelkeies" ref="*" querytype="xml">
        <template>
          <query expr="mousewheelkey">
            <assign var="?action" expr="concat('mousewheel.',@name,'.action')"/>
            <assign var="?delta_multiplier_x" expr="concat('mousewheel.',@name,'.delta_multiplier_x')"/>
            <assign var="?delta_multiplier_y" expr="concat('mousewheel.',@name,'.delta_multiplier_y')"/>
            <assign var="?delta_multiplier_z" expr="concat('mousewheel.',@name,'.delta_multiplier_z')"/>
          </query>
          <action>
            <tabpanel uri="?">
              <grid>
                <columns><column/><column/><column/></columns>
                <rows>
                  <row align="center">
                    <label value="&ui.mousewheel.binding.action.label;"/>
                    <menulist preference="?action"><menupopup>
                      <menuitem value="0" label="&ui.mousewheel.binding.action.do_nothing.label;"/>
                      <menuitem value="1" label="&ui.mousewheel.binding.action.scroll.label;"/>
                      <menuitem value="2" label="&ui.mousewheel.binding.action.go_history.label;"/>
                      <menuitem value="3" label="&ui.mousewheel.binding.action.zoom.label;"/>
                    </menupopup></menulist>
                  </row>
                  <row align="center">
                    <label value="&ui.mousewheel.binding.delta_multiplier_x.label;"/>
                    <textbox type="number" preference="?delta_multiplier_x"/>
                    <label value="%"/>
                  </row>
                  <row align="center">
                    <label value="&ui.mousewheel.binding.delta_multiplier_y.label;"/>
                    <textbox type="number" preference="?delta_multiplier_y"/>
                    <label value="%"/>
                  </row>
                  <row align="center">
                    <label value="&ui.mousewheel.binding.delta_multiplier_z.label;"/>
                    <textbox type="number" preference="?delta_multiplier_z"/>
                    <label value="%"/>
                  </row>
                </rows>
              </grid>
            </tabpanel>
          </action>
        </template>
      </tabpanels>
    </tabbox>
  </groupbox>

</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-multitouch">
  <groupbox id="multitouch-gestures-box">
    <caption label="&ui.multitouch.gestures.label;"/>
    <hbox>
      <vbox flex="1">
        <label value="&ui.multitouch.gestures.pinch.in.label;" control="menu_pinch_in"/>
        <menulist class="indent" editable="true" preference="browser.gesture.pinch.in" id="menu_pinch_in"/>
        <label value="&ui.multitouch.gestures.pinch.in.shift.label;" control="menu_pinch_in_shift"/>
        <menulist class="indent" editable="true" preference="browser.gesture.pinch.in.shift" id="menu_pinch_in_shift"/>
      </vbox>
      <vbox flex="1">
        <label value="&ui.multitouch.gestures.pinch.out.label;" control="menu_pinch_out"/>
        <menulist class="indent" editable="true" preference="browser.gesture.pinch.out" id="menu_pinch_out"/>
        <label value="&ui.multitouch.gestures.pinch.out.shift.label;" control="menu_pinch_out_shift"/>
        <menulist class="indent" editable="true" preference="browser.gesture.pinch.out.shift" id="menu_pinch_out_shift"/>
      </vbox>
    </hbox>
    <groupbox>
      <caption label="&ui.multitouch.gestures.swipe.label;"/>
      <grid>
      <columns><column flex="1"/><column flex="1"/><column flex="1"/></columns>
      <rows>
      <row align="center">
        <box/>
        <menulist editable="true" preference="browser.gesture.swipe.up"/>
        <box/>
      </row>
      <row align="center">
        <menulist editable="true" preference="browser.gesture.swipe.left"/>
        <box/>
        <menulist editable="true" preference="browser.gesture.swipe.right"/>
      </row>
      <row align="center">
        <box/>
        <menulist editable="true" preference="browser.gesture.swipe.down"/>
        <box/>
      </row>
      </rows>
      </grid>
    </groupbox>
    <groupbox orient="horizontal">
      <caption label="&ui.multitouch.gestures.twist.label;"/>
      <menulist editable="true" preference="browser.gesture.twist.left" flex="1"/>
      <menulist editable="true" preference="browser.gesture.twist.right" flex="1"/>
    </groupbox>
  </groupbox>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-findahead">
  <groupbox>
    <caption label="&ui.find.findahead.label;"/>
    <description>&ui.find.findahead.desc;</description>
    <hbox align="center">
      <label value="&ui.find.findahead.timeout.label;" control="keyNavTypeAHeadTimeout"/>
      <textbox id="keyNavTypeAHeadTimeout" type="number" hidespinbuttons="true" preference="accessibility.typeaheadfind.timeout" size="5"/>
      <label value="&ui.find.findahead.timeout.label2;"/>
    </hbox>
    <checkbox id="keyNavTypeAHeadSound" label="&ui.find.findahead.sound.label;"
     preference="accessibility.typeaheadfind.enablesound"/>
  </groupbox>
  <groupbox>
    <caption label="&ui.find.casesensitive.label;"/>
    <radiogroup preference="accessibility.typeaheadfind.casesensitive">
      <radio value="0" label="&ui.find.casesensitive.insensitive;"/>
      <radio value="1" label="&ui.find.casesensitive.sensitive;"/>
      <radio value="-1" label="&ui.find.casesensitive.auto;"/>
    </radiogroup>
  </groupbox>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-keynav">
  <groupbox>
    <caption label="&ui.keynav.tabNavigationBehavior.label;"/>
    <vbox>
      <checkbox id="tabNavigationLinks" preference="accessibility.tabfocus"
                label="&ui.keynav.tabNavigationLinks.label;"
                accesskey="&ui.keynav.tabNavigationLinks.accesskey;"
                onsyncfrompreference="return gPrefWindow.prefUI.onTabNavSyncFrom(this);"
                onsynctopreference="return gPrefWindow.prefUI.onTabNavSyncTo(this);"/>
      <checkbox id="tabNavigationForms" preference="accessibility.tabfocus"
                label="&ui.keynav.tabNavigationForms.label;"
                accesskey="&ui.keynav.tabNavigationForms.accesskey;"
                onsyncfrompreference="return gPrefWindow.prefUI.onTabNavSyncFrom(this);"
                onsynctopreference="return gPrefWindow.prefUI.onTabNavSyncTo(this);"/>
    </vbox>
    <description>&ui.keynav.tabNavigationTextboxes.label;</description>
  </groupbox>

  <groupbox align="start">
    <caption label="&ui.keynav.browseWithCaret.label;"/>
    <checkbox label="&ui.keynav.browseWithCaret.shortcut.label;"
              accesskey="&ui.keynav.browseWithCaret.shortcut.accesskey;"
              preference="accessibility.browsewithcaret_shortcut.enabled"
              onsyncfrompreference="gPrefWindow.prefUI.onBrowseWithCaretSyncFrom();"/>
    <checkbox label="&ui.keynav.browseWithCaret.warn.label;"
              accesskey="&ui.keynav.browseWithCaret.warn.accesskey;"
              class="indent"
              preference="accessibility.warn_on_browsewithcaret"/>
  </groupbox>

  <groupbox>
    <caption label="&ui.keynav.accelerators.label;"/>
    <label value="&ui.keynav.acceleratorsBlurb.label;"/>

    <html:div>
      <label value="&ui.keynav.acceleratorKey.label;"
       accesskey="&ui.keynav.acceleratorKey.accesskey;" control="acceleratorKeyValue"/>
      <textbox id="acceleratorKeyValue" type="number" hidespinbuttons="true" preference="ui.key.accelKey" size="5"/>
    </html:div>
    <html:div>
      <label value="&ui.keynav.menuAccessKey.label;" accesskey="&ui.keynav.menuAccessKey.accesskey;" control="menuAccessKeyValue"/>
      <textbox id="menuAccessKeyValue" type="number" hidespinbuttons="true" preference="ui.key.menuAccessKey" size="5"/>
    </html:div>
  </groupbox>

  <groupbox>
    <caption label="&ui.keynav.backspace_action.label;"/>
    <radiogroup preference="browser.backspace_action">
      <radio value="0" label="&ui.keynav.backspace_action.goBack;"/>
      <radio value="1" label="&ui.keynav.backspace_action.scrollUp;"/>
      <radio value="2" label="&ui.keynav.backspace_action.nothing;"/>
    </radiogroup>
  </groupbox>

  <groupbox>
    <caption label="&ui.keynav.accesibility.label;"/>
    <checkbox label="&ui.keynav.accesibility.accesskeycauseactivation.label;"
              accesskey="&ui.keynav.accesibility.accesskeycauseactivation.accesskey;"
              preference="accessibility.accesskeycausesactivation"/>
  </groupbox>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-bidi">
  <checkbox label="&ui.bidi.ui.label;" preference="bidi.browser.ui"/>
  <grid>
    <columns><column/><column flex="1"/></columns>
    <rows>
      <row align="center">
        <label value="&ui.bidi.direction.label;"/>
        <menulist id="bidi-direction" preference="bidi.direction"><menupopup>
          <menuitem value="1" label="&ui.bidi.direction.ltr.label;"/>
          <menuitem value="2" label="&ui.bidi.direction.rtl.label;"/>
        </menupopup></menulist>
      </row>
      <row align="center">
        <label control="bidi-numeral" value="&ui.bidi.numeral.label;"/>
        <menulist id="bidi-numeral" preference="bidi.numeral"><menupopup>
          <menuitem value="0" label="&ui.bidi.numeral.nominal.label;"/>
          <menuitem value="1" label="&ui.bidi.numeral.regular_context.label;"/>
          <menuitem value="2" label="&ui.bidi.numeral.hindi_context.label;"/>
          <menuitem value="3" label="&ui.bidi.numeral.arabic.label;"/>
          <menuitem value="4" label="&ui.bidi.numeral.hindi.label;"/>
          <menuitem value="5" label="&ui.bidi.numeral.persian_context.label;"/>
          <menuitem value="6" label="&ui.bidi.numeral.persian.label;"/>
        </menupopup></menulist>
      </row>
      <row align="center">
        <label value="&ui.bidi.support.label;"/>
        <menulist id="bidi-provider" preference="bidi.support"><menupopup>
          <menuitem value="1" label="&ui.bidi.support.mozilla.label;"/>
          <menuitem value="2" label="&ui.bidi.support.os.label;"/>
          <menuitem value="3" label="&ui.bidi.support.disable.label;"/>
        </menupopup></menulist>
      </row>
      <row align="center">
        <label value="&ui.bidi.texttype.label;"/>
        <menulist id="bidi-texttype" preference="bidi.texttype"><menupopup>
          <menuitem value="1" label="&ui.bidi.texttype.charset.label;"/>
          <menuitem value="2" label="&ui.bidi.texttype.logical.label;"/>
          <menuitem value="3" label="&ui.bidi.texttype.visual.label;"/>
        </menupopup></menulist>
      </row>
    </rows>
  </grid>
  <groupbox>
    <caption label="&ui.bidi.scrollbarSide.label;"/>
    <radiogroup preference="layout.scrollbar.side">
      <radio value="0" label="&ui.bidi.scrollbarSide.uiBase;"/>
      <radio value="1" label="&ui.bidi.scrollbarSide.contentBase;"/>
      <radio value="2" label="&ui.bidi.scrollbarSide.right;"/>
      <radio value="3" label="&ui.bidi.scrollbarSide.left;"/>
    </radiogroup>
  </groupbox>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-i18n">
  <groupbox>
    <hbox align="center">
      <label value="&ui.i18n.locale.label;" control="contentLocale"/>
      <textbox id="contentLocale" preference="general.useragent.locale" size="8"/>
      <label value="&ui.i18n.locale.eg;"/>
    </hbox>
    <checkbox class="indent" label="&ui.i18n.locale.matchOS.label;" preference="intl.locale.matchOS" onsyncfrompreference="gPrefWindow.prefUI.onLocaleMatchOSSyncFrom(this)"/>
  </groupbox>

  <groupbox>
    <caption label="&ui.i18n.spellcheckDefault.label;"/>
    <radiogroup preference="layout.spellcheckDefault">
      <radio value="0" label="&ui.i18n.spellcheckDefault.disabled;"/>
      <radio value="1" label="&ui.i18n.spellcheckDefault.multiline;"/>
      <radio value="2" label="&ui.i18n.spellcheckDefault.all;"/>
    </radiogroup>
  </groupbox>
  <checkbox label="&ui.i18n.japanese_specific_transform.label;" preference="layout.enable_japanese_specific_transform"/>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-contextmenu">
  <checkbox label="&ui.contextmenu.click_hold_context_menus.label;"
            preference="ui.click_hold_context_menus"/>
  <groupbox>
    <caption label="&ui.contextmenu.uiSubmenuDelay.label;"/>
    <radiogroup id="uiSubmenuDelay-radio">
      <radio value="0" label="&ui.contextmenu.uiSubmenuDelay.none.label;"/>
      <radio value="-1" label="&ui.contextmenu.uiSubmenuDelay.inf.label;"/>
      <hbox align="center">
        <radio value="*" label="&manualSet;"/>
        <textbox id="uiSubmenuDelay" type="number" hidespinbuttons="true" min="-1" size="4" preference="ui.submenuDelay"
          onsyncfrompreference="return gPrefWindow.radioGrConnSyncFrom('uiSubmenuDelay-radio','uiSubmenuDelay',0);"/>
        <label value="&miliseconds;" id="uiSubmenuDelay-ms"/>
      </hbox>
    </radiogroup>
  </groupbox>
</box>
<box class="subtabpanel" orient="vertical" id="ui-pane-others">
  <groupbox>
    <caption label="&ui.others.preference.label;"/>
    <checkbox label="&ui.others.preference.animateFadeIn.label;" preference="browser.preferences.animateFadeIn"/>
    <checkbox label="&ui.others.preference.inContent.label;" preference="browser.preferences.inContent"
              onsyncfrompreference="gPrefWindow.prefUI.onPreferenceInContentSyncFrom();"/>
    <vbox id="ui-pane-others-pref-instanceAccept" class="indent">
      <checkbox label="&ui.others.preference.instanceAccept.label;" preference="browser.preferences.instantApply"/>
      <description class="indent">&ui.others.preference.instanceAccept.desc;</description>
    </vbox>
  </groupbox>
  <groupbox>
    <caption label="&ui.others.xwin.label;"/>
    <checkbox label="&ui.others.xwin.autocopy.label;" preference="clipboard.autocopy"/>
    <checkbox label="&ui.others.xwin.paste.label;" preference="middlemouse.paste"/>
    <checkbox label="&ui.others.xwin.contentLoadURL.label;" preference="middlemouse.contentLoadURL"/>
    <checkbox label="&ui.others.xwin.scrollbarPosition.label;" preference="middlemouse.scrollbarPosition"/>
  </groupbox>
  <groupbox>
    <caption label="&ui.others.word_select.label;"/>
    <checkbox label="&ui.others.word_select.eat_space_to_next_word.label;" preference="layout.word_select.eat_space_to_next_word"/>
    <checkbox label="&ui.others.word_select.eat_punctuation.label;" preference="layout.word_select.stop_at_punctuation"/>
  </groupbox>
  <checkbox label="&ui.others.enable_drag_images.label;" preference="nglayout.enable_drag_images"/>
  <checkbox label="&ui.others.xul_file_picker;" preference="ui.allow_platform_file_picker"/>
</box>
</deck>
</hbox>

</prefpane>
</overlay>
