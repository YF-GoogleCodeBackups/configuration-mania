  <preference id="middlemouse.paste" name="middlemouse.paste" type="bool"/>
  <preference id="middlemouse.contentLoadURL" name="middlemouse.contentLoadURL" type="bool"/>
  <preference id="middlemouse.scrollbarPosition" name="middlemouse.scrollbarPosition" type="bool"/>
